AddKeysToAgent yes
Compression yes

Host *
	Match host * exec "nc -z %h.belfodil.duckdns.org 22 -w 1"
		HostName %h.belfodil.duckdns.org

Host rp-1 rp-2 rp-3 aurelius diogenes descartes aristotle
	User ubuntu

Host diogenes-bmc
	User root
	HostName diogenes.belfodil.duckdns.org
	KexAlgorithms +diffie-hellman-group1-sha1
	HostKeyAlgorithms +ssh-rsa

Host h500i
	User anes

Host h500i-boot
	User root
	HostName h500i.belfodil.duckdns.org
