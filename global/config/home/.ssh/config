AddKeysToAgent yes
Compression yes

Host *
	Match exec "echo %h | grep -Eq '^[A-Za-z0-9_-]+$' && nc -z %h.local.network 22 -w 1"
		HostName %h.local.network

Host rp-1 rp-2 rp-3 aurelius diogenes descartes aristotle
	User ubuntu

Host h500i
	User anes

Host h500i-boot
	User root
	HostName h500i.belfodil.duckdns.org
